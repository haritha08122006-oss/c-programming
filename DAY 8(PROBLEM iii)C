#include<stdio.h>
#include<stdlib.h>
struct contact{
    int number;
    struct contact*next;
};
int main()
{
    struct contact*ct1 = (struct contact*)malloc(sizeof(struct contact));
    struct contact*ct2 = (struct contact*)malloc(sizeof(struct contact));
    struct contact*ct3 = (struct contact*)malloc(sizeof(struct contact));
    
    

 scanf("%d",&ct1->number);
      scanf("%d",&ct2->number);
      scanf("%d",&ct3->number);
     
     
     ct1->next = ct2;
     ct2->next = ct3;
     ct3->next = NULL;
     
   
     
     struct contact *head = ct1;
     printf("Contact: \n");
     while(head != NULL){
         printf("%d----->",head->number);
         head = head->next;
     }
         printf("NULL");
     
     return 0;
     }